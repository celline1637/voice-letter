{
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "preserve",
  "importOrder": [
    "^@/redux/(.*)$",
    "^@/hooks",
    "^@/hooks/(.*)$",
    "^@/mocks/(.*)$",
    "^@/apis/(.*)$",
    "^@/services",
    "^@/services/(.*)$",
    "^@/controllers",
    "^@/controllers/(.*)$",
    "^@/lib/(.*)$",
    "^@/config/(.*)$",
    "^@/shared/(.*)$",
    "^@/theme/(.*)$",
    "^@/pages",
    "^@/pages/(.*)$",
    "^@/components/(.*)$",
    "^@/images/(.*)$",
    "^@/types/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
